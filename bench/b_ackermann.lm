# Benchmark: Ackermann function
# Tests: deep recursion, stack depth, call overhead
# Expected result: ack(3, 8) = 2045

cell ack(m: Int, n: Int) -> Int
  if m == 0
    n + 1
  else
    if n == 0
      ack(m - 1, 1)
    else
      ack(m - 1, ack(m, n - 1))
    end
  end
end

cell main() -> Int
  ack(3, 8)
end
