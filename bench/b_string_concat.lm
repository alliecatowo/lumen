# Benchmark: String concatenation
# Tests: string allocation, copying, garbage pressure, length builtin
# Expected result: length = 500,000

cell build_string(n: Int) -> String
  let mut s = ""
  let mut i = 0
  while i < n
    s = s + "hello"
    i = i + 1
  end
  s
end

cell main() -> Int
  let s = build_string(100000)
  length(s)
end
