cell sum_to_n(n: Int) -> Int
  let mut sum = 0
  let mut i = 0
  while i < n
    sum = sum + i
    i = i + 1
  end
  return sum
end

cell factorial(n: Int) -> Int
  if n <= 1
    return 1
  end
  return n * factorial(n - 1)
end

cell main() -> Int
  let x = sum_to_n(10)
  let y = factorial(5)
  return x + y
end
