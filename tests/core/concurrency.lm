async cell simple_async() -> Int
  return 42
end

cell test_async_cell() -> Bool
  let future = simple_async()
  let result = await future
  return result == 42
end

cell main() -> Null
  assert test_async_cell()
  print("=== All Concurrency Tests Passed! ===")
  return null
end
