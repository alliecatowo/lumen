{
  "name": "tree-sitter-lumen",
  "version": "0.3,0",
  "description": "Tree-sitter grammar for the Lumen programming language",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "tree-sitter",
    "parser",
    "lumen",
    "ai-native",
    "programming-language"
  ],
  "author": "Lumen Contributors",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/alliecatowo/lumen.git"
  },
  "dependencies": {
    "node-addon-api": "^8.0.0",
    "node-gyp-build": "^4.8.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.24.0",
    "prebuildify": "^6.0.0"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.0"
  },
  "peerDependenciesMeta": {
    "tree_sitter": {
      "optional": true
    }
  },
  "tree-sitter": [
    {
      "scope": "source.lumen",
      "file-types": [
        "lm",
        "lumen"
      ],
      "injection-regex": "^lumen$",
      "highlights": "queries/highlights.scm",
      "locals": "queries/locals.scm"
    }
  ],
  "scripts": {
    "test": "tree-sitter test",
    "build": "tree-sitter generate && node-gyp-build",
    "parse": "tree-sitter parse"
  },
  "files": [
    "grammar.js",
    "binding.gyp",
    "bindings/node/*",
    "queries/*",
    "src/*"
  ]
}
